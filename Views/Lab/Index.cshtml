@model IEnumerable<Lab>;

<div>
    <a asp-action="Create" class="btn btn-success">Criar</a>
@if(Model.Count() > 0) 
{
        <ul>
            @foreach(var lab in Model)
            {
                <li>ID = @lab.ID, Nome = @lab.Name, Bloco = @lab.Block, Numero = @lab.Number <a asp-action="Show" asp-route-id="@lab.ID">Ver mais</a></li>
            }
        </ul>
}
else 
{
    <h1>Não há laboratórios</h1>
}
</div>
